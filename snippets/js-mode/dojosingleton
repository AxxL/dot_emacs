// @formatter:off
define(["dojo/_base/declare",
        "dojo/parser"],
function(declare,
         parser) {
    // @formatter:on
    return {
        _href : null,
        startup : function() {         
            parser.parse();
            this._href = window.location.href;
        }$0
    };
});
